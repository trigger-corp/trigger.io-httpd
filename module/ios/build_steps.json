[
    { "do": { "add_ios_system_framework": "CFNetwork.framework" } },
    { "do": { "add_ios_system_framework": "MobileCoreServices.framework" } },
    { "do": { "add_ios_system_framework": "libz.dylib" } },
    {
        "do": {
            "ios_configure_ats": [
                {
                    "domain": "localhost",
                    "NSExceptionAllowsInsecureHTTPLoads": true,
                    "NSExceptionRequiresForwardSecrecy": true
                }
            ]
        }
    },
    { "do": { "set_in_info_plist": { "key": "CRHTTPConnectionReadHeaderTimeout",   "value":  60 } } },
    { "do": { "set_in_info_plist": { "key": "CRHTTPConnectionReadBodyTimeout",     "value":  60 } } },
    { "do": { "set_in_info_plist": { "key": "CRConnectionKeepAliveTimeout",        "value": 120 } } },
    { "do": { "set_in_info_plist": { "key": "CRConnectionReadTimeoutKey",          "value":  60 } } },
    { "do": { "set_in_info_plist": { "key": "CRConnectionWriteTimeoutKey",         "value":  60 } } },
    { "do": { "set_in_info_plist": { "key": "CRConnectionMaxKeepAliveConnections", "value":  50 } } }
]
